{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Startup": {
    "MiniProfiler": true
  },

  /* JWT字符串生成参数 */
  "JWTPart": {
    "Secret": "CloudWhales123456abcdefg123456Secret",
    "SecretFile": "C:\\pro-file\\jwt.part.secret.txt",
    "Issuer": "CloudWhales",
    "Audience": "BlogUser"
  },

  /* 数据库参数配置 */
  "DataBaseSetting": {
    "Author": "CloudWhales",
    "UseLoadTest": false,

    //生成数据库表结构
    "SeedDBFrameEnabled": true,

    //生成数据表格内容
    "SeedDBDataEnabled": true,

    //当前项目的主库标识，对应数据库的字符串中Enabled必须为true
    "MainDB": "DB_SQLServer",

    //是否开始多库模式
    "MultiDBEnabled": false,

    //是否开启读写分离，必须为单库模式,数据库类型一致
    "CQRSEnabled": false
  },
  /* 数据库连接字符串配置 */
  "DBS": [

    {},
    {},
    {},
    {},
    {}
  ]
}
